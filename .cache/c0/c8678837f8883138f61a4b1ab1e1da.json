ing: optimizeSpeed;\n   line-height: 1.5;\n}\n\n/* A elements that don't have a class get default styles */\na:not([class]) {\n   text-decoration-skip-ink: auto;\n}\n\n/* Make images easier to work with */\nimg,\npicture {\n   max-width: 100%;\n   display: block;\n}\n\n/* Inherit fonts for inputs and buttons */\ninput,\nbutton,\ntextarea,\nselect {\n   font: inherit;\n}\n\n/* Remove all animations and transitions for people that prefer not to see them */\n@media (prefers-reduced-motion: reduce) {\n   html:focus-within {\n      scroll-behavior: auto;\n   }\n   *,\n   *::before,\n   *::after {\n      animation-duration: 0.01ms !important;\n      animation-iteration-count: 1 !important;\n      transition-duration: 0.01ms !important;\n      scroll-behavior: auto !important;\n   }\n}\n","styles/base/_base.scss":"ul,\nli {\n   margin: 0;\n   padding: 0;\n}\n\na {\n   text-decoration: none;\n}\n\nbutton {\n   background-color: transparent;\n   border: none;\n   outline: 1px solid;\n   &:hover {\n      cursor: pointer;\n   }\n}\n\ninput {\n   border: none;\n   outline: 1px solid;\n}\n\n.hidden {\n   display: none;\n}\n","styles/base/_typography.scss":"@use \"abstracts\" as a;\n\nbody {\n   font: 1.2rem 'Rubik', sans-serif;\n}\n","styles/layout/_stickyFooter.scss":"@use \"abstracts\" as *;\n\nbody {\n   @include flexParent(column, unset, unset);\n   min-height: 100vh;\n}\n\nmain {\n   flex-grow: 1;\n}\n","styles/abstracts/_mixins.scss":"@use \"maps\" as *;\n\n@mixin flexParent($direction, $justify, $align) {\n   display: flex;\n   flex-direction: $direction;\n   justify-content: $justify;\n   align-items: $align;\n}\n\n@mixin flexChild($grow, $basis) {\n   flex-grow: $grow;\n   flex-basis: $basis;\n}\n\n@mixin position($position, $width, $height, $top, $left) {\n   position: $position;\n   width: $width;\n   height: $height;\n   top: $top;\n   left: $left;\n}\n\n@mixin positionCentered($width, $height) {\n   position: absolute;\n   width: $width;\n   height: $height;\n   inset: 0;\n   margin: auto;\n}\n\n@mixin btnLink($bgColor, $color, $borderColor, $hoverColor) {\n   padding: 0.5em;\n   background-color: $bgColor;\n   color: $color;\n   border-radius: 0.4rem;\n   box-shadow: 0 0 0.4rem 0 rgba(black, 0.5);\n\n   &:hover {\n      background-color: white;\n      border: 0.2rem solid $borderColor;\n      color: $hoverColor;\n   }\n}\n\n@mixin after($content, $width, $bottom) {\n   &::after {\n      content: $content;\n      display: absolute;\n      width: $width;\n      height: 0.2rem;\n      bottom: $bottom;\n      left: 50%;\n      transform: translateX(-50%);\n   }\n}\n\n@mixin transition() {\n   transition: 300ms ease-in-out;\n}\n\n@mixin mq($key) {\n   @media (min-width: map-get($breakpoints, $key)) {\n      @content;\n   }\n}\n\n@mixin fq() {\n   @supports (font-size: clamp) {\n      @content;\n   }\n}\n","styles/layout/_alignment.scss":"@use \"abstracts\" as *;\n\n.alignment-pt {\n   @include flexParent(column, center, center);\n}\n\n.alignment {\n   width: 90%;\n   max-width: 60rem;\n   outline: 1px solid red;\n}\n","styles/layout/_header.scss":"@use \"abstracts\" as *;\n\n.hdr {\n   outline: 1px solid;\n   background-color: lightgray;\n   padding-block: 2rem;\n}\n\n.hdr_cnt {\n   @include flexParent(row, space-between, center);\n}\n\n.hdr_mobile-nav-ct {\n   @include position(absolute, unset, unset, 0, 0);\n   min-width: 100%;\n   min-height: 100%;\n   background-color: rgba($fcPm, 0.9);\n   padding-top: 2rem;\n   z-index: 100;\n}\n\n.hdr_mobile-nav {\n   width: 80%;\n   margin: 0 auto;\n}\n\n.mb-nav-hdr {\n   @include flexParent(row, space-between, center);\n   margin-bottom: 1.3rem;\n}\n\n.hdr_nav-mb {\n   & li a {\n      border-bottom: 0.1px solid rgba(white, 0.2);\n   }\n   & a {\n      display: block;\n      width: 100%;\n      color: white;\n      text-transform: uppercase;\n      letter-spacing: 0.2ch;\n      text-align: center;\n      padding-block: 0.7rem;\n      @include transition();\n      &:first-child {\n         border-top: 1px solid rgba(white, 0.2);\n      }\n      &:hover {\n         background-color: white;\n         color: $fcPm;\n      }\n   }\n}\n\n.login-mb {\n   border: 2px solid white;\n   border-radius: 0.4rem;\n   margin-top: 1.5rem;\n}\n","styles/abstracts/_variables.scss":"$softBlue: hsl(231, 69%, 60%);\n$softRed: hsl(0, 94%, 66%);\n$fcPm: hsl(229, 31%, 21%);\n$fcSd: hsl(229, 8%, 60%);\n$fcTr: white;\n","styles/layout/_footer.scss":".ftr {\n   background-color: lightgray;\n}\n\n.svg-refs {\n   display: none;\n}\n"},"lineCount":null}},"error":null,"hash":"369507677cf59e44aad5cd72054b5b38","cacheData":{"env":{}}}